    "length=int(input())\n",
    "List=list(map(int,input().split()))\n",
    "even_list=list()\n",
    "odd_list=list()\n",
    "for j in List:\n",
    "    if(j%2==0):\n",
    "        even_list.append(j)\n",
    "    elif(j%2!=0):\n",
    "        odd_list.append(j)\n",
    "if(len(even_list)>len(odd_list)):\n",
    "    print(\"READY FOR BATTLE\")\n",
    "else:\n",
    "    print(\"NOT READY\")"
